     <%= form_for :article, url: articles_path do |f| %>

    <%= render 'layouts/errorcheck', {article: @article} %>

     <p>
        <%= f.label :subject %><br>
        <%= f.text_field :subject %>
      </p>
       <p>
         <%= f.label :text %><br>
         <%= f.text_area :text %>
       </p><p>
         <%= f.submit %>
       </p>
     <% end %>