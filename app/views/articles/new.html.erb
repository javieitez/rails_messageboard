     <%= form_for :article, url: articles_path do |f| %>
     <p>
        <%= f.label :subject %><br>
        <%= f.text_field :subject, size: 83 %>
      </p>
       <p>
         <%= f.label :text %><br>
         <%= f.text_area :text, cols: 80, rows: 12 %>
       </p><p>
         <%= f.submit %>
       </p>
     <% end %>